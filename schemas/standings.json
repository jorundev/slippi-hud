{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"placement": {
				"type": "number",
				"default": -1
			},
			"name": {
				"type": "string",
				"default": ""
			},
			"country": {
				"type": "string",
				"default": ""
			},
			"isTeam": {
				"type": "boolean",
				"default": false
			},
			"character": {
				"type": "string",
				"default": ""
			},
			"twitter": {
				"type": "string",
				"default": ""
			}
		}
	},
	"default": []
}
